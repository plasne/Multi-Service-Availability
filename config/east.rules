[
    {
        "name": "db-failover",
        "if": "!west-db:up && !db:write",
        "then": {
            "pause": 20000,
            "perform-only-if-unchanged": true, 
            "action": {
                "method": "POST",
                "uri": "http://pelasne-nginx-east.eastus.cloudapp.azure.com:82/failover"
            }
        }
    }
]